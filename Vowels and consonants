#include <iostream>

using namespace std;

int main()
{
    char alpha;
    cout << "Enter any alphabet\n";
    cin >> alpha;
    switch(alpha)
    {
    case 'a':
        cout << alpha << " is a vowel\n";
        break;

    case 'e':
        cout << alpha << " is a vowel\n";
        break;

    case 'i':
        cout << alpha << " is a vowel\n";
        break;

    case 'o':
        cout << alpha << " is a vowel\n";
        break;

    case 'u':
        cout << alpha << " is a vowel\n";
        break;

    default:
        cout << alpha << " is a consonant\n";
    }
}
