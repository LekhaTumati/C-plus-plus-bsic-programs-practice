#include <iostream>
#include<math.h>
using namespace std;

int main()
{
    float a, b, c;
    cout << "Enter a, b, c\n";
    cin >> a >> b >> c;
    float s = (a + b + c)/2;
    float area = sqrt(s*(s-a)*(s-b)*(s-c));
    cout << "The area of the triangle using heron's formula is " << area;
    return 0;
}
